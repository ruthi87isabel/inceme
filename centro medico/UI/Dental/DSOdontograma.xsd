                      <Parameter AllowDbNull="true" AutogeneratedName="Odontograma" ColumnName="IDOdontograma" DataSourceName="inceme.dbo.d_Acciones" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@Odontograma" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="IDOdontograma" SourceColumnNullMapping="false" SourceVersion="Current" />
