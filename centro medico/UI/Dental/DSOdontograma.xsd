FROM            d_Presupuesto AS D INNER JOIN
                         d_PresupuestoLineas AS L ON D.IDPresupuesto = L.IDPresupuesto INNER JOIN
                         CONCEPTOSFRA AS C ON C.CODIGO = L.IDTratamiento INNER JOIN
                         d_Odontogramas AS O ON D.IDOdontograma = O.IDOdontograma INNER JOIN
                         PACIENTES AS P ON O.IDPaciente = P.CPACIENTE
WHERE        (D.IDPresupuesto = @IDPresupuesto)
ORDER BY L.IDPresupuestoLinea</CommandText>
