                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      SELECT     A.FechaEjecucion, C.DESCRIPCION AS Tratamiento, A.PiezasInvolucradas, A.Cuadrantes,
                      COALESCE(P.NOMBRE,'') + ' ' + COALESCE(P.APELLIDO1,'') + ' ' + COALESCE(P.APELLIDO2,'') AS Paciente
                      FROM         d_Acciones AS A INNER JOIN
                      CONCEPTOSFRA AS C ON C.CODIGO = A.IDTratamiento INNER JOIN
                      d_Odontogramas AS O ON A.IDOdontograma = O.IDOdontograma INNER JOIN
                      PACIENTES AS P ON O.IDPaciente = P.CPACIENTE
                      WHERE     (A.IDOdontograma = @Odontograma)
                      ORDER BY A.FechaEjecucion, A.IDAccion</CommandText>
